{
	"compilerOptions": {
		"module": "esnext",
		"lib": ["dom", "esnext"],
		"declaration": true,
		"sourceMap": true,
		"removeComments": false,
		"allowSyntheticDefaultImports": true,
		"target": "es6",
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"jsx": "react",
		"forceConsistentCasingInFileNames": true
	},
	"exclude": ["node_modules", "dist"]
}
